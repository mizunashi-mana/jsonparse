{
  "name": "sonparser",
  "version": "0.0.1",
  "description": "Safe parser of JSON and CSON for config like ConfigParser of python",
  "main": "./build/index.js",
  "scripts": {
    "prepublish": "gulp build --production",
    "postinstall": "dtsm --config ./dtsm.json install",
    "clean": "gulp clean",
    "lint": "gulp lint",
    "build": "gulp build --production",
    "test": "gulp test",
    "devBuild": "gulp lint && gulp build --debug",
    "dinstall": "dtsm --config ./dtsm.json install --save",
    "dupdate": "dtsm --config ./dtsm.json update --save"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mizunashi-mana/jsonparse.git"
  },
  "keywords": [
    "json",
    "parser"
  ],
  "author": "Mizunashi Mana <mizunashi_mana@mma.club.uec.ac.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mizunashi-mana/jsonparse/issues"
  },
  "homepage": "https://github.com/mizunashi-mana/jsonparse#readme",
  "devDependencies": {
    "chai": "^3.3.0",
    "coffee-script": "^1.10.0",
    "dtsm": "^0.11.0",
    "gulp": "^3.9.0",
    "gulp-coffeelint": "^0.5.0",
    "gulp-eslint": "^1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-header": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-mocha": "^2.1.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^3.3.0",
    "gulp-typescript": "^2.9.2",
    "merge2": "^0.3.6",
    "mocha": "^2.3.3",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.3.2",
    "typescript": "^1.6.2",
    "yargs": "^3.27.0"
  },
  "dependencies": {
    "cson": "^3.0.2"
  }
}
