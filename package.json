{
  "name": "sonparser",
  "version": "0.1.1",
  "description": "Safe parser of JSON and CSON for config like ConfigParser of python",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "dtsm --config ./dtsm.json install && gulp build --production",
    "clean": "gulp clean",
    "remove": "gulp remove",
    "lint": "gulp lint",
    "build": "gulp build --production",
    "doc": "gulp build:doc",
    "test": "gulp lint && gulp test --production",
    "devBuild": "gulp lint && gulp build --debug",
    "devTest": "gulp test --debug",
    "dinstall": "dtsm --config ./dtsm.json install --save",
    "dupdate": "dtsm --config ./dtsm.json update --save"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mizunashi-mana/node-sonparser.git"
  },
  "keywords": [
    "json",
    "cson",
    "type",
    "parser",
    "parse",
    "config",
    "strict",
    "configparser"
  ],
  "author": {
    "name": "Mizunashi Mana",
    "email": "mizunashi_mana@mma.club.uec.ac.jp"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mizunashi-mana/node-sonparser/issues"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "homepage": "https://github.com/mizunashi-mana/node-sonparser#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "coffee-script": "^1.10.0",
    "dtsm": "^0.13.0",
    "gulp": "^3.9.0",
    "gulp-coffeelint": "^0.5.0",
    "gulp-data": "^1.2.0",
    "gulp-eslint": "^1.1.0",
    "gulp-filter": "^3.0.1",
    "gulp-header": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-mocha": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^3.6.0",
    "gulp-typedoc": "^1.2.1",
    "gulp-typescript": "^2.9.2",
    "gulp-util": "^3.0.7",
    "merge2": "^0.3.6",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.4",
    "object-assign": "^4.0.1",
    "require-reload": "^0.2.2",
    "rimraf": "^2.4.4",
    "run-sequence": "^1.1.5",
    "source-map-support": "^0.3.3",
    "typescript": "^1.6.2",
    "yargs": "^3.30.0"
  },
  "dependencies": {
    "cson": "^3.0.2",
    "lodash": "^3.10.1"
  }
}
